                        "UseCase",
                        "User",
                        "Vulnerability",
                        "Zone"
                    ],
                    "default": "All"
                },
                "query": {
                    "data_type": "string",
                    "order": 1,
                    "description": "Query Text",
                    "required": true,
                    "primary": true,
                    "contains": [
                        "arcsight search string"
