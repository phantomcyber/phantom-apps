    "actions": [
        {
            "action": "test connectivity",
            "description": "This action runs a lookup ip action to test connection",
            "type": "test",
            "identifier": "test_connectivity",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup ip",
            "description": "Attempts a lookup of the hostname for the provided IP",
            "type": "investigate",
            "identifier": "lookup_ip",
            "read_only": true,
            "parameters": {
                "ip": {
                    "data_type": "string",
                    "order": 0,
                    "description": "IP to resolve",
                    "contains": [
                        "ip"
                    ],
                    "required": true,
                    "primary": true
                },
                "port": {
                    "data_type": "numeric",
                    "order": 1,
                    "description": "Port number",
                    "required": false
                },
                "timeout": {
                    "data_type": "numeric",
                    "order": 2,
                    "description": "Seconds before timeout (Default: 30)",
                    "required": false
                }
            },
            "output": [
                {
                    "data_path": "action_result.data.*.hostnames.*",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.status",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ]
                },
                {
                    "data_path": "action_result.parameter.port",
                    "data_type": "numeric"
                },
                {
                    "data_path": "action_result.parameter.timeout",
                    "data_type": "numeric"
                }
            ],
            "versions": "EQ(*)"
        }
    ]
