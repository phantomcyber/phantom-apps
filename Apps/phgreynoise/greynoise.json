    "configuration": {
        "api_key": {
            "description": "API Key for GreyNoise",
            "data_type": "password",
            "required": true,
            "order": 0
        },
        "on_poll_query": {
            "description": "GNQL query to use for the on poll action",
            "data_type": "string",
            "default": "Please refer to the documentation",
            "order": 1
        },
        "on_poll_size": {
            "description": "The number of results to return for the on poll action",
            "data_type": "numeric",
            "default": 25,
            "order": 2
        }
