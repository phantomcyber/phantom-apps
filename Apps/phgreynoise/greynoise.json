    "actions": [
        {
            "action": "test connectivity",
            "description": "Validate the asset configuration for connectivity using the supplied configuration",
            "verbose": "Tests the connection to the GreyNoise API.",
            "type": "test",
            "identifier": "test_connectivity",
            "read_only": true,
            "parameters": {},
            "output": [],
            "versions": "EQ(*)"
        },
        {
            "action": "lookup ip",
            "description": "Lookup IP using GreyNoise API Quick Check Endpoint",
            "type": "investigate",
            "identifier": "lookup_ip",
            "read_only": true,
            "parameters": {
                "ip": {
                    "description": "IP to query",
                    "data_type": "string",
                    "contains": [
                      "ip"
                    ],
                    "primary": true,
                    "required": true,
                    "order": 0
                }
            },
            "output": [
                {
                    "data_path": "action_result.parameter.ip",
                    "data_type": "string",
                    "contains": [
                        "ip"
                    ],
                    "example_values": [
                        "71.6.135.131"
                    ]
                },
                {
                  "data_path": "action_result.data.*.code",
                  "data_type": "string",
                  "contains": [
                      "greynoise code"
                  ],
                  "example_values": [
                      "0x00",
                      "0x01"
                  ]
                },
                {
                    "data_path": "action_result.data.*.code_meaning",
                    "data_type": "string",
                    "contains": [
                        "greynoise code meaning"
                    ],
                    "example_values": [
                        "The IP has never been observed scanning the Internet",
                        "The IP has been observed by the GreyNoise sensor network"
                    ]
                },
                {
                    "data_path": "action_result.data.*.noise",
                    "data_type": "boolean",
                    "contains": [
                        "greynoise noise"
                    ],
                    "example_values": [
                        true,
                        false
                    ]
                },
                {
                    "data_path": "action_result.status",
                    "data_type": "string",
                    "example_values": [
                        "success",
                        "failed"
                    ]
                },
                {
                    "data_path": "action_result.message",
                    "data_type": "string"
                },
                {
                    "data_path": "action_result.summary",
                    "data_type": "string"
                },
                {
                    "data_path": "summary.total_objects",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                },
                {
                    "data_path": "summary.total_objects_successful",
                    "data_type": "numeric",
                    "example_values": [
                        1
                    ]
                }  
            ],
            "render": {
                "type": "custom",
                "view": "greynoise_view.report",
                "title": "GreyNoise IP Lookup"
            },
            "versions": "EQ(*)"
        },
        {
            "action": "ip reputation",
            "description": "Get full GreyNoise reputation and context for a specific IP",
