    {
      "action": "create privacy incident",
      "identifier": "create_privacy_incident",
      "description": "Create Radar Privacy Incident",
      "verbose": "This action creates a privacy incident in Radar using Phantom settings and asset configurations. The Base URL For Phantom Appliance configured on the Company Settings page is used to link the incident's \"intake method\" to this instance of Phantom. The incident group ID in the asset settings is used for authorization. The time zone specified in the asset settings is used in combination with the current moment the action is triggered to set the incident's \"discovered\" date and time. The container ID for the associated Phantom event or case is used to specify the Radar incident channel ID. This is used to associate the Radar incident with the Phantom container. Action parameters are used to set the Radar incident name and description.",
      "type": "generic",
      "read_only": false,
      "parameters": {
        "name": {
          "description": "A name summarizing the privacy incident",
          "data_type": "string",
          "required": true,
          "default": "",
          "order": 0
        },
        "description": {
          "description": "A description of the privacy incident",
          "data_type": "string",
          "order": 1
        }
      },
      "output": [
        {
          "data_path": "action_result.data.*.incident_id",
          "data_type": "numeric",
          "column_name": "id",
          "column_order": 0,
          "contains": [
            "radar incident id"
          ]
        },
        {
          "data_path": "action_result.data.*.name",
          "data_type": "string",
          "column_name": "name",
          "column_order": 1
        },
        {
          "data_path": "action_result.data.*.description",
          "data_type": "string",
          "column_name": "description",
          "column_order": 2
        },
        {
          "data_path": "action_result.data.*.discovered",
          "data_type": "string",
          "column_name": "discovered at",
          "column_order": 3
        },
        {
          "data_path": "action_result.data.*.url",
          "data_type": "string",
          "column_name": "url",
          "column_order": 4
        },
        {
          "data_path": "action_result.status",
          "data_type": "string",
          "column_name": "status",
          "column_order": 5
        },
        {
          "data_path": "action_result.message",
          "data_type": "string",
          "column_order": 6,
          "example_values": [
            "Successfully created a privacy incident"
          ]
        }
      ],
      "render": {
        "type": "custom",
        "view": "radar_view.display_view",
        "title": "Privacy Incident",
        "menu_name": "Radar Privacy Incident"
      },
      "versions": "EQ(*)"
    },
