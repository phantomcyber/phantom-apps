                    "contains": [
                        "irondefense alert id"
                    ],
                    "order": 0
                },
                "analyst_severity": {
                    "description": "The severity of the alert",
                    "data_type": "string",
                    "required": true,
                    "value_list": [
                        "undecided",
                        "benign",
                        "suspicious",
                        "malicious"
                    ],
                    "default": "undecided",
                    "order": 1
