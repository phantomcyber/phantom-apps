                "comment": {
                    "description": "Text of comment",
                    "data_type": "string",
                    "required": true,
                    "order": 3
                },
                "share_comment_with_irondome": {
                    "description": "Shares the provided comment with IronDome",
                    "data_type": "boolean",
                    "default": true,
                    "order": 4
